<%= form_with model: @post do |form| %>
  <p>
    <%= form.label "name", "Post name" %>
    <%= form.text_field "name" %>
    </p>
    <p>
    <%= form.label "date", "Date of Visit" %>
    <%= form.text_field "date" %>
     </p>
    <p>
    <%= form.label "Description" %>
    <%= form.text_field "description" %>
     </p>
     <%= form.hidden_field "place_id"%>
  <%= form.submit %>
<% end %>